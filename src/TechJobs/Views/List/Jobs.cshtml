<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{ 
     
}

@foreach (Dictionary<string, string> job in ViewBag.jobs)
{
    <table class="job-listing">
        <tbody>
            @foreach (KeyValuePair<string, string> jobList in job)
            {
                <tr>
                    <td>@jobList.Key</td>
                    <td>@jobList.Value</td>
                </tr>
            }
        </tbody>
    </table>

}